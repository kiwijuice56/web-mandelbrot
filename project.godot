config_version=4

_global_script_classes=[ {
"base": "ColorRect",
"class": "FractalNavigation",
"language": "GDScript",
"path": "res://main/fractal_navigation.gd"
} ]
_global_script_class_icons={
"FractalNavigation": ""
}

[application]

config/name="mandelbrot-fractal"
run/main_scene="res://main/FractalNavigation.tscn"
